<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.kylekelley.votemaryland">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registration_webview"
            android:label="@string/title_activity_registration_webview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".candidate_list"
            android:label="@string/title_activity_candidate_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Calander_Of_Events" />
        <activity android:name=".FindMyPollingPlace" />
        <activity android:name=".FAQ" />
        <activity
            android:name=".EventDetail"
            android:parentActivityName=".Calander_Of_Events"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Calander_Of_Events" />
        </activity>
        <activity android:name=".CandidateDetail" />
        <activity android:name=".fav_page"></activity>
    </application>

</manifest>
