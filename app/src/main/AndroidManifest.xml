<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.kylekelley.votemaryland">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registration_webview"
            android:label="@string/title_activity_registration_webview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".candidate_list"
            android:label="@string/title_activity_candidate_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calendar_Of_Events"
            android:label="@string/title_activity_calender__of__events" />
        <activity
            android:name=".FindMyPollingPlace"
            android:label="@string/title_activity_polling_place" />
        <activity
            android:name=".FAQ"
            android:label="@string/title_activity_faq" />
        <activity android:name=".EventDetail" />
        <activity android:name=".CandidateDetail" />
    </application>

</manifest>
